     <p v-show=\"number2 != 0\">{{ number1 / number2 }}</p>\n        </div>\n    </div>\n    <div>\n        <input type=\"text\" v-model=\"user.first_name\" placeholder=\"Primeiro nome\">\n        <input type=\"text\" v-model=\"user.last_name\" placeholder=\"Ultimo nome\"><br><br>\n        {{ user.first_name }} {{ user.last_name }}\n    </div>\n</template>\n<script>\n    export default{\n        name: 'App',\n        data() {\n            return {\n                name: '',\n                pageCount: 5,\n                number1: null,\n                number2: null,\n                user: {\n                    first_name: 'Guilherme',\n                    last_name: 'Valença'\n                }\n            }\n        },\n        watch: {\n            name(newValue, oldValue) {\n                    this.askGetServer(newValue,oldValue);\n            },\n            pageCount() {\n                this.changePage();\n            },\n            user: {\n                handler() {\n                    console.log('changed user')\n                },\n                deep: true\n            }\n        },\n        computed: {\n\n        },\n        methods: {\n            askGetServer(newValue,oldValue) {\n                alert(`nome antigo:${oldValue}, nome novo:${newValue}`);\n            },\n            changePage() {\n                console.log('realizando ajax');\n            }\n        }\n    }\n</script>"],"mappings":"AAmCI,eAAc;EACVA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHD,IAAI,EAAE,EAAE;MACRE,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE;QACFC,UAAU,EAAE,WAAW;QACvBC,SAAS,EAAE;MACf;IACJ;EACJ,CAAC;EACDC,KAAK,EAAE;IACHR,IAAIA,CAACS,QAAQ,EAAEC,QAAQ,EAAE;MACjB,IAAI,CAACC,YAAY,CAACF,QAAQ,EAACC,QAAQ,CAAC;IAC5C,CAAC;IACDR,SAASA,CAAA,EAAG;MACR,IAAI,CAACU,UAAU,CAAC,CAAC;IACrB,CAAC;IACDP,IAAI,EAAE;MACFQ,OAAOA,CAAA,EAAG;QACNC,OAAO,CAACC,GAAG,CAAC,cAAc;MAC9B,CAAC;MACDC,IAAI,EAAE;IACV;EACJ,CAAC;EACDC,QAAQ,EAAE,CAEV,CAAC;EACDC,OAAO,EAAE